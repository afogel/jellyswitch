<% title "Usage" %>

<%= breadcrumb do %>
  <li class="breadcrumb-item"><%= link_to @user.name, user_path(@user) %></li>
  <li class="breadcrumb-item active" aria-current="page">Usage</li>
<% end %>

<div class="row mt-0 mb-0">
  <div class="col-lg-3"></div>
  <div class="col-lg-6 m-0 pl-0 pr-0">
    <%= list_wrapper do %>
      <%= list_item do %>
        <%= render "operator/users/usage", user: @user, usage_report: @usage_report %>
      <% end %>
    <% end %>
  </div>
  <div class="col-lg-3"></div>
</div>